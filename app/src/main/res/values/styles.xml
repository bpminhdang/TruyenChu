<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ImageViewShape">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="AppTheme" parent="Theme.Material3.DynamicColors.DayNight">
        <!-- Other attributes -->
    </style>



    <style name="CustomBottomNavIndicator" parent="@style/Widget.MaterialComponents.BottomNavigationView">
        <!-- Các thuộc tính khác của chỉ báo khi được chọn nếu cần -->
    </style>


    <style name="favorite">
        .material-symbols-outlined {
        font-variation-settings:
        'FILL' 0,
        'wght' 400,
        'GRAD' 0,
        'opsz' 24
        }</style>

    <style name="top_navigation_custom_avs">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@drawable/rounded_top_background</item>
        <item name="android:elevation">8dp</item>
    </style>

    <style name="RoundBorderDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@drawable/round_border_background</item>
        <item name="android:windowMinWidthMajor">90%</item>
        <item name="android:windowMinWidthMinor">90%</item>
        <item name="android:windowIsFloating">true</item>
    </style>



</resources>